    "word = input()\n",
    "total = 0\n",
    "for i in range(len(word)):\n",
    "    if i % 3 == 0 and word[i] != 'P':\n",
    "        total += 1\n",
    "    if i % 3 == 1 and word[i] != \"E\":\n",
    "        total += 1\n",
    "    if i % 3 == 2 and word[i] != \"R\":\n",
    "        total += 1\n",
    "print(total)\n"
   ]
