    "num = int(input())\n",
    "height = 1\n",
    "used  = 1\n",
    "length = 1\n",
    "width = 1\n",
    "while(True):\n",
    "    length += 2\n",
    "    width += 2\n",
    "    used += length * width\n",
    "    if used > num:\n",
    "        break\n",
    "    height += 1\n",
    "print(height)"
